{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "name": "Gaia",
    "slug": "gaia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dylan.vpa.gaia"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptative-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dylan.vpa.gaia"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c57f3849-015f-40ed-92e1-83adf512ccd9"
      }
    },
    "owner": "dylan.vpa"
  }
}
